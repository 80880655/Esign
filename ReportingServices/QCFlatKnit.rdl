    <DataSet Name="dsFinishing">
      <Query>
        <DataSourceName>escmQC</DataSourceName>
        <QueryParameters>
          <QueryParameter Name=":QualityCode">
            <Value>=Parameters!QualityCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT A.FINISHING_CODE,B.FINISHING_NAME,B.DESCRIPTION FROM QCFinishDtl A, pbKnitFinish B 
WHERE A.FINISHING_CODE=B.FINISHING_CODE AND A.QUALITY_CODE=(:QualityCode)</CommandText>
      </Query>
      <Fields>
        <Field Name="FINISHING_CODE">
