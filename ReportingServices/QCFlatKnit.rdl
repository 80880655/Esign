    <DataSet Name="dsQCYarn">
      <Query>
        <DataSourceName>escmQC</DataSourceName>
        <QueryParameters>
          <QueryParameter Name=":QualityCode">
            <Value>=Parameters!QualityCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT B.DESCRIPTION||'-&gt;'||A.YARN_TYPE as YARNTYPE, A.THREADS,A.YARN_TYPE, A.YARN_RATIO, A.YARN_COUNT,A.YARN_COMPONENT 
FROM QCYarnDtl A,pbKnitYarnType B 
WHERE A.YARN_TYPE=B.YARN_TYPE
AND A.QUALITY_CODE=(:QualityCode)</CommandText>
      </Query>
      <Fields>
        <Field Name="YARNTYPE">
          <DataField>YARNTYPE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
