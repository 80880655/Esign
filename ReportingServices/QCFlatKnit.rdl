        </Field>
        <Field Name="YARN_RATIO">
          <DataField>YARN_RATIO</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="YARN_TYPE">
          <DataField>YARN_TYPE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="YARN_COMPONENT">
          <DataField>YARN_COMPONENT</DataField>
          <rd:TypeName>System.String</rd:TypeName>
