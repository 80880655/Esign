    <DataSet Name="dsApprovedFPPO">
      <Query>
        <DataSourceName>escmQC</DataSourceName>
        <QueryParameters>
          <QueryParameter Name=":QualityCode">
            <Value>=Parameters!QualityCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT distinct pi.ppo_no||'('|| pi.FABRIC_TYPE_CD||')'  as PPO 
FROM ppo_hd ph,gen_customer c,ppo_qcmaininfo qc, ppo_item pi, fab_combo fc, ppo_qccomment qcc,ppo_item_combo pic,QCMainInfo qm  
WHERE ph.ppo_no=pi.ppo_no and  fc.fab_combo_id = qcc.fab_combo_id and qc.ppo_item_id=pi.ppo_item_id 
and pi.ppo_item_id=pic.ppo_item_id and pi.quality_code=qm.quality_code and 
pic.fab_combo_id=fc.fab_combo_id and qc.ppo_qc_id=qcc.ppo_qc_id and ph.customer_cd=c.customer_cd and qm.Status='Approved' 
and QCC.STATUS='Approved' and qm.quality_code=(:QualityCode)</CommandText>
      </Query>
      <Fields>
        <Field Name="PPO">
          <DataField>PPO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
