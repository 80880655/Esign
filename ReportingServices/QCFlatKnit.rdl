    <DataSet Name="dsCustomer_comment">
      <Query>
        <DataSourceName>escmQC</DataSourceName>
        <QueryParameters>
          <QueryParameter Name=":QualityCode">
            <Value>=Parameters!QualityCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT  qcc.customer_comment
       FROM ppo_hd ph,gen_customer c,ppo_qcmaininfo qc, ppo_item pi, fab_combo fc, ppo_qccomment qcc,ppo_item_combo pic,QCMainInfo qm  
        WHERE ph.ppo_no=pi.ppo_no and  fc.fab_combo_id = qcc.fab_combo_id and qc.ppo_item_id=pi.ppo_item_id and pi.ppo_item_id=pic.ppo_item_id and pi.quality_code=qm.quality_code and 
               pic.fab_combo_id=fc.fab_combo_id and qc.ppo_qc_id=qcc.ppo_qc_id and ph.customer_cd=c.customer_cd 
               And ph.status&lt;&gt;'C' And qc.status&lt;&gt;'C' And pi.status&lt;&gt;'C' And qcc.status&lt;&gt;'C' And pic.status&lt;&gt;'C'
               and qm.Quality_Code=(:QualityCode)</CommandText>
      </Query>
      <Fields>
        <Field Name="CUSTOMER_COMMENT">
          <DataField>CUSTOMER_COMMENT</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
